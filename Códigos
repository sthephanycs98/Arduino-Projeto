Cooler Acionado: 

var num = msg.payload; 
if (num >= 22){
msg.payload = true; }
else{
msg.payload = false;
}
return msg;


Boolean
var string = msg.payload;
if (string == "true"){
msg.payload = true; }
else{
msg.payload = false; } 
